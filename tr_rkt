alter table raw.dwh.tr_rkt
add (period_budget_new date);

update raw.dwh.tr_rkt
set period_budget_new = substring(period_budget,1,10)::date
where period_budget_new is null;

alter table raw.dwh.tr_rkt
drop column period_budget;

alter table raw.dwh.tr_rkt
rename column period_budget_new to period_budget;
